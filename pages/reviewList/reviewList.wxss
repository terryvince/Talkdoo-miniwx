/* pages/reviewList/reviewList.wxss */
page{
  background: rgba(234,239,243,0.30);
}

.rv-item{
  padding:34rpx 32rpx;
  background: white;
  margin: 32rpx;
  box-shadow:0px 4rpx 8rpx 0px rgba(0,0,0,0.12);
  border-radius:16rpx;
  min-height: 170rpx;
}
.rv-item.bg-green{
  background: rgba(143,195,31,1);
}
.rv-item.bg-green text,.rv-item.bg-green view{
  color: white!important;
}